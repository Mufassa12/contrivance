  "query": "\n            SELECT id, email, password_hash, name, role as \"role: UserRole\", created_at, updated_at, is_active, last_login\n            FROM users \n            WHERE is_active = true \n              AND (LOWER(name) LIKE $1 OR LOWER(email) LIKE $1)\n            ORDER BY name\n            LIMIT $2\n            ",
